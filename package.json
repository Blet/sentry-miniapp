{
  "name": "sentry-miniapp",
  "version": "0.8.2",
  "description": "用于小程序平台的 Sentry SDK",
  "repository": "git://github.com/lizhiyao/sentry-miniapp.git",
  "homepage": "https://github.com/lizhiyao/sentry-miniapp",
  "miniprogram": "dist",
  "main": "dist/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:dev": "npm run build:wx && npm run build:my && npm run build:tt",
    "build:wx": "webpack --config ./webpack.config.wx.js",
    "build:my": "webpack --config ./webpack.config.my.js",
    "build:tt": "webpack --config ./webpack.config.tt.js",
    "build": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "version": "node ./scripts/versionbump.js src/version.ts"
  },
  "keywords": [
    "sentry",
    "weapp",
    "miniapp"
  ],
  "author": "dancerlzy@gmail.com",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@sentry/typescript": "^5.3.0",
    "@types/node": "^12.7.1",
    "miniprogram-api-typings": "^2.7.7-2",
    "replace-in-file": "^4.1.3",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@sentry/core": "^5.6.1",
    "@sentry/types": "^5.6.1",
    "@sentry/utils": "^5.6.1",
    "tslib": "^1.10.0"
  }
}